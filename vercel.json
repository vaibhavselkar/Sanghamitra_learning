{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "cd frontend && npm install",
        "buildCommand": "cd frontend && npm start",  
        "devCommand": "cd frontend && npm start",    
        "outputDirectory": "frontend"            
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/" 
    }
  ]
}
